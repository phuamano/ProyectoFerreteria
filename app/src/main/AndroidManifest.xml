<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reservando.proyectoferreteria">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ferreteria"
        android:label="Ferreteria"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProductoActivity"
            android:label="@string/title_activity_producto"></activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"></activity>
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu"></activity>
        <activity
            android:name=".ClienteActivity"
            android:label="@string/title_activity_cliente"></activity>
        <activity
            android:name=".VentaActivity"
            android:label="@string/title_activity_venta"></activity>
        <activity android:name=".SignupActivity"></activity>
    </application>

</manifest>
